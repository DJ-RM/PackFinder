    "bluebird": "^3.5.1",
    "body-parser": "^1.16.1",
    "chance": "^1.0.16",
    "compression": "^1.7.1",
    "connect-session-sequelize": "^4.1.0",
    "css-loader": "^0.28.11",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "geolib": "^2.0.24",
    "history": "^4.6.3",
    "moment": "^2.22.1",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.8",
    "react": "16.0.0-alpha.12",
    "react-animations": "^1.0.0",
    "react-big-calendar": "^0.19.0",
    "react-d3-basic": "^1.6.11",
    "react-d3-core": "^1.3.9",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.0.0",
    "react-google-maps": "^9.4.5",
